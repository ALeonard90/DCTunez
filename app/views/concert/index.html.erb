<div class="events">


</div>

<script>
	var request= $.ajax({
		url:"/concerts",
		type:"GET",
		dataType:"json",
		success:function(response){
			console.log(response);
			for (var i=0;i<response.length;i++) {
            $('.events').append('<div class="headliner">' + response[i].headliner +'</div>')
            $('.events').append('<div class="tunez"><iframe src="https://embed.spotify.com/?uri=spotify%3Aartist%3A'+response[i].playlist+'"width="300" height="380" frameborder="0" allowtransparency="true"></iframe>')
            // alert(response[i].headliner);
            
         }	
		}
	})
</script>